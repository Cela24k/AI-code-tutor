{
  "manifest_version": 3,
  "name": "Code Extension",
  "description": "React Chrome ChatGPT Extension",
  "version": "1.0.1",
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open popup"
  },
  "permissions": ["scripting", "tabs", "storage"]
}